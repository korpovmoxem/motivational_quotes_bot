button_1 = KeyboardButton(text='Подписаться на рассылку')
button_2 = KeyboardButton(text='Получить цитату дня')
button_3 = KeyboardButton(text='Отписаться от рассылки')

keyboard = ReplyKeyBoardMarkup(keyboard=[[button_1, button_2, button_3]], resize_keyboard=True)

@dp.message(CommandStart())
async def process_start_command(message: Message)
